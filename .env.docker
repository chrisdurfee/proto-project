# Environment configuration for Docker containers
# Copy this to .env and modify as needed

# Database Configuration
DB_HOST=mariadb
DB_DATABASE=proto
DB_USERNAME=proto_user
DB_PASSWORD=proto_password
DB_PORT=3306

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# Application Configuration
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost:8080

# MariaDB Root Password (for container setup)
MARIADB_ROOT_PASSWORD=root

# Timezone
TZ=America/Denver
