{
	"name": "protoframework/proto-project",
	"description": "This is a project that uses the Proto Framework.",
	"keywords": [
		"proto",
		"framework",
		"modular",
		"monolith",
		"php"
	],
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "Chris Durfee",
			"email": "j.c.durf@gmail.com"
		}
	],
	"require": {
		"php": ">=8.1",
		"protoframework/proto": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.0",
		"fakerphp/faker": "^1.21",
		"phpstan/phpstan": "^1.10"
	},
	"scripts": {

	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Modules\\": "modules/",
			"Common\\": "common/"
		},
		"classmap": [
			"common/Migrations",
			"modules/*/Migrations"
		]
	}
}